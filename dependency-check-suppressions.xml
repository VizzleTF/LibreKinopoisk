<?xml version="1.0" encoding="UTF-8"?>
<suppressions xmlns="https://jeremylong.github.io/DependencyCheck/dependency-suppression.1.3.xsd">
    <!-- Исключения для ложноположительных уязвимостей -->
    
    <!-- Android Support Libraries - usually false positives -->
    <suppress>
        <notes><![CDATA[
        Ложноположительное срабатывание для Android Support Libraries
        ]]></notes>
        <packageUrl regex="true">^pkg:maven/androidx\..*</packageUrl>
        <cve>CVE-2018-9492</cve>
    </suppress>
    
    <!-- OkHttp false positives -->
    <suppress>
        <notes><![CDATA[
        OkHttp библиотека используется в актуальной версии
        ]]></notes>
        <packageUrl regex="true">^pkg:maven/com\.squareup\.okhttp3/okhttp@.*</packageUrl>
        <vulnerabilityName>CVE-2023-3635</vulnerabilityName>
    </suppress>
    
    <!-- Kotlin false positives -->
    <suppress>
        <notes><![CDATA[
        Kotlin stdlib ложноположительные срабатывания
        ]]></notes>
        <packageUrl regex="true">^pkg:maven/org\.jetbrains\.kotlin/.*</packageUrl>
        <cve>CVE-2020-15250</cve>
    </suppress>
</suppressions> 